{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "blob.v0.dev"
      }
    ]
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
